<UserControl x:Class="RC_FE_Design___Analysis_and_synthesis.FEEditor.Elements.PageControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:controls="clr-namespace:RC_FE_Design___Analysis_and_synthesis.FEEditor.Controls"
             mc:Ignorable="d">

    <Canvas Width="1500" Height="900" ClipToBounds="False" Background="{DynamicResource LogicTransparentColorKey}">
        <Rectangle Width="{Binding RelativeSource={RelativeSource Mode=FindAncestor,AncestorType={x:Type Canvas}}, Path=Width}"
                   Height="{Binding RelativeSource={RelativeSource Mode=FindAncestor,AncestorType={x:Type Canvas}}, Path=Height}"
                   Stroke="Black" StrokeThickness="2"/>
    </Canvas>

</UserControl>
